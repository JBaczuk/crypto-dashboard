{
  "name": "crypto-dashboard",
  "version": "0.1.0",
  "api_version": "0.1.0",
  "description": "Open source dashboard for managing cryptocurrency portfolio",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "test_balances": "babel-node src/models/test_balances.js --presets es2015,stage-0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JBaczuk/crypto-dashboard"
  },
  "author": "Jordan Baczuk <jordan.baczuk@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "coinbase": "^2.0.6",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "gdax": "^0.4.2",
    "morgan": "^1.8.0",
    "node-bittrex-api": "^0.8.1",
    "poloniex.js": "^0.0.7",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "bugs": {
    "url": "https://github.com/JBaczuk/crypto-dashboard"
  },
  "homepage": "https://github.com/JBaczuk/crypto-dashboard"
}
